@model IEnumerable<CarInsurance.Models.Table>

@{ 
    ViewBag.Title = "Admin View";
}

<h2>Admin View - All Quotes</h2>

<table class="table">
    <tr>
        <th> First Name</th>
        <th> last Name</th>
        <th> Email Address</th>
        <th> Quote</th>
    </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td>@item.FirstName</td>
        <td>@item.LastName</td>
        <td>@item.EmailAddress</td>
        <td>@item.Quote</td>
    </tr>
    }
</table>
